{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Launch EC2 - CSYE 6225 Summer 2019",
    "Resources": {
        "myVPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "10.0.0.0/16",
                "EnableDnsSupport": "true",
                "EnableDnsHostnames": "true",
                "InstanceTenancy": "default",
                "Tags": [
                    {
                        "Key": "foo",
                        "Value": "bar"
                    }
                ]
            }
        },
        "IPv6CidrBlock" : {
            "Type" : "AWS::EC2::VPCCidrBlock",
            "Properties" : {
              "AmazonProvidedIpv6CidrBlock" : true,
              "VpcId" : { "Ref" : "myVPC" }
            }
          },
          "subnet1" : {
            "Type" : "AWS::EC2::Subnet",
            "DependsOn" : "IPv6CidrBlock",
            "Properties" : {
              "AvailabilityZone": "us-east-1a",
              "AssignIpv6AddressOnCreation" : true,
              "CidrBlock" : { "Fn::Select" : [ 0, { "Fn::Cidr" : [{ "Fn::GetAtt" : [ "myVPC", "CidrBlock" ]}, 3, 8 ]}]},
              "Ipv6CidrBlock" : { "Fn::Select" : [ 0, { "Fn::Cidr" : [{ "Fn::Select" : [ 0, { "Fn::GetAtt" : [ "myVPC", "Ipv6CidrBlocks" ]}]}, 3, 64 ]}]},
              "VpcId" : { "Ref" : "myVPC" }
            }
          },
          "subnet2" : {
            "Type" : "AWS::EC2::Subnet",
            "DependsOn" : "IPv6CidrBlock",
            "Properties" : {
              "AvailabilityZone": "us-east-1b",
              "AssignIpv6AddressOnCreation" : true,
              "CidrBlock" : { "Fn::Select" : [ 1, { "Fn::Cidr" : [{ "Fn::GetAtt" : [ "myVPC", "CidrBlock" ]}, 3, 8 ]}]},
              "Ipv6CidrBlock" : { "Fn::Select" : [ 1, { "Fn::Cidr" : [{ "Fn::Select" : [ 0, { "Fn::GetAtt" : [ "myVPC", "Ipv6CidrBlocks" ]}]}, 3, 64 ]}]},
              "VpcId" : { "Ref" : "myVPC" }
            }
          },
          "subnet3" : {
            "Type" : "AWS::EC2::Subnet",
            "DependsOn" : "IPv6CidrBlock",
            "Properties" : {
              "AvailabilityZone": "us-east-1c",
              "AssignIpv6AddressOnCreation" : true,
              "CidrBlock" : { "Fn::Select" : [ 2, { "Fn::Cidr" : [{ "Fn::GetAtt" : [ "myVPC", "CidrBlock" ]}, 3, 8 ]}]},
              "Ipv6CidrBlock" : { "Fn::Select" : [ 2, { "Fn::Cidr" : [{ "Fn::Select" : [ 0, { "Fn::GetAtt" : [ "myVPC", "Ipv6CidrBlocks" ]}]}, 3, 64 ]}]},
              "VpcId" : { "Ref" : "myVPC" }
            }
          },
          "myInternetGateway" : {
            "Type" : "AWS::EC2::InternetGateway",
            "Properties" : {
              "Tags" : [ {"Key" : "key1", "Value" : "value1"}]
            }
         },
         "AttachGateway" : {
            "Type" : "AWS::EC2::VPCGatewayAttachment",
            "Properties" : {
               "VpcId" : { "Ref" : "myVPC" },
                "InternetGatewayId" : { "Ref" : "myInternetGateway" }
             }
         },
         "myRouteTable" : {
            "Type" : "AWS::EC2::RouteTable",
            "Properties" : {
               "VpcId" : { "Ref" : "myVPC" },
               "Tags" : [ { "Key" : "name", "Value" : "Public" } ]
            }
         },
         "PublicRoute": {
            "Type": "AWS::EC2::Route",
            "DependsOn": ["AttachGateway","myRouteTable"],
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "GatewayId": "myInternetGateway",
                "RouteTableId": "myRouteTable"
            }
        },
         "Subnet1RouteTableAssociation" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
               "SubnetId" : { "Ref" : "subnet1" },
               "RouteTableId" : { "Ref" : "myRouteTable" }
            }
         },
         "Subnet2RouteTableAssociation" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
               "SubnetId" : { "Ref" : "subnet2" },
               "RouteTableId" : { "Ref" : "myRouteTable" }
            }
         },
         "Subnet3RouteTableAssociation" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
               "SubnetId" : { "Ref" : "subnet3" },
               "RouteTableId" : { "Ref" : "myRouteTable" }
            }
         }
    },
    "Outputs": {
        "CidrBlock": {
            "Description": "The set of IP addresses for the VPC.",
            "Value": {
                "Fn::GetAtt": [
                    "myVPC",
                    "CidrBlock"
                ]
            }
        },
        "DefaultNetworkAcl": {
            "Description": "The default network ACL ID that is associated with the VPC. ",
            "Value": {
                "Fn::GetAtt": [
                    "myVPC",
                    "DefaultNetworkAcl"
                ]
            }
        },
        "DefaultSecurityGroup": {
            "Description": "The default security group ID that is associated with the VPC.",
            "Value": {
                "Fn::GetAtt": [
                    "myVPC",
                    "DefaultSecurityGroup"
                ]
            }
        }
    }
}
